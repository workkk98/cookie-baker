{
  "name": "cookie-baker",
  "version": "2.0.0",
  "author": "workkk",
  "description": "manage cookie easily and conveniently.",
  "icons": {
    "16": "cookie.png",
    "48": "cookie.png",
    "128": "cookie.png"
  },
  "permissions": [
    "cookies",
    "http://*/*",
    "https://*/*",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "cookie.png",
    "default_title": "Cookie Baker"
  },
  "devtools_page": "devtools.html",
  "manifest_version": 2
}